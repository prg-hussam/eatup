{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-debugbar": "^3.9",
        "guzzlehttp/guzzle": "^7.5",
        "inertiajs/inertia-laravel": "^0.6.9",
        "jenssegers/agent": "^2.6.4",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^10.13.5",
        "laravel/sanctum": "^3.2.1",
        "laravel/tinker": "^2.8",
        "mcamara/laravel-localization": "^1.7",
        "nwidart/laravel-modules": "^9.0.6",
        "prgayman/laravel-sidebar": "dev-develop",
        "propaganistas/laravel-phone": "^5.1",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-permission": "^5.8",
        "spatie/laravel-translatable": "^6.3",
        "swayok/alternative-laravel-cache": "^6.1.8",
        "swayok/cache-filesystem-adapter": "*",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "beyondcode/laravel-query-detector": "^1.8",
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.19.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "prgayman/laravel-sidebar": {
            "type": "path",
            "url": "packages/sidebar",
            "options": {
                "symlink": true
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "laravel/fortify"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
